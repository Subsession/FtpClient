{
  "name": "comertis/ftp",
  "description": "A object-oriented FTP wrapper library",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "readme": "README.md",
  "prefer-stable": true,
  "support": {
    "docs": "https://github.com/Comertis/FtpClient/tree/master/docs",
    "issues": "https://github.com/Comertis/FtpClient/issues",
    "source": "https://github.com/Comertis/FtpClient"
  },
  "authors": [
    {
      "name": "Cristian Moraru",
      "email": "cristian@comertis.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Comertis\\Ftp\\": "src/",
      "Comertis\\Ftp\\Internal\\": "src/internal",
      "Comertis\\Ftp\\Internal\\Files\\": "src/internal/files",
      "Comertis\\Ftp\\Internal\\Connection\\": "src/internal/connection"
    },
    "exclude-from-classmap": [
      "/test/",
      "/tests/",
      "/Test/",
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Comertis\\Ftp\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  }
}
